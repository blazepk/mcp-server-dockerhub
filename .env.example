# Docker Hub Authentication
DOCKERHUB_USERNAME=your_username
DOCKERHUB_PASSWORD=your_token

# Optional: Private Registry Configuration
PRIVATE_REGISTRY_URL=https://your-registry.com
PRIVATE_REGISTRY_USERNAME=your_username
PRIVATE_REGISTRY_PASSWORD=your_token

# Cache Configuration
CACHE_TTL_SECONDS=300
CACHE_MAX_SIZE=1000

# Rate Limiting
RATE_LIMIT_REQUESTS_PER_MINUTE=100
RATE_LIMIT_BURST_SIZE=10

# Logging
LOG_LEVEL=info

# MCP Server Configuration
MCP_SERVER_NAME=dockerhub-mcp-server
MCP_SERVER_VERSION=1.0.0
